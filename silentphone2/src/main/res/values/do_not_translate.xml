    <string name="s4_c016_explanation" translatable="false">May indicate a software error, or a possible attempted attack. Close this call and setup a new call.</string>
    <string name="s4_c020" translatable="false">Critical software error</string>
    <string name="s4_c020_explanation" translatable="false">Internal application error. Close this call, restart the app and setup a new call.</string>
